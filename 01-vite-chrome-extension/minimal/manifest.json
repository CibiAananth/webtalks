{
  "manifest_version": 3,
  "name": "Minimal Manifest",
  "version": "1.0.1",
  "action": {
    "default_popup": "./src/popup/index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}
